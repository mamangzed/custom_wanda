<!-- custom_nama/views/import_so_lines_wizard_view.xml -->

<odoo>
    <record id="view_import_so_lines_wizard_form" model="ir.ui.view">
        <field name="name">import.so.lines.wizard.form</field>
        <field name="model">import.so.lines.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="file" widget="binary" filename="file_name"/>
                </group>
                <footer>
                    <button string="Import" class="oe_highlight" type="object" name="action_import_so_lines"/>
                    <button string="Cancel" class="oe_secondary" onclick="window.location.reload();"/>
                    <button string="Download Template" class="oe_highlight" type="object" name="action_download_template"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
